   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/latex": [
       "$$\\frac{C b \\sqrt{\\frac{S b}{b + 2}}}{2 \\left(b + 2\\right)} \\left(3 b + 4\\right)$$"
      ],
      "text/plain": [
       "        _______          \n",
       "       ╱  S⋅b            \n",
       "C⋅b⋅  ╱  ───── ⋅(3⋅b + 4)\n",
       "    ╲╱   b + 2           \n",
       "─────────────────────────\n",
       "        2⋅(b + 2)        "
      ]
     },
     "execution_count": 8,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "A = b*y\n",
    "P = b + 2*y\n",
    "R = A/P\n",
    "Q = A*C*sqrt(R*S)\n",
    "\n",
    "simplify(Q.diff(y).subs(y, 1))"
   ]
