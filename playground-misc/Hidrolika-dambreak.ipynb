   "source": [
    "import numpy as np\n",
    "\n",
    "dx = 1\n",
    "lf = 10\n",
    "imax = int(lf/dx)+1\n",
    "p_temp = np.ones((imax))\n",
    "p_up = -1\n",
    "slope = abs(p_up)/(lf)\n",
    "# slope = 1/100\n",
    "\n",
    "x = 0\n",
    "for i in range(imax):\n",
    "    p_temp[i] = p_up + slope*x\n",
    "    x = x + dx\n",
    "    \n",
    "print(p_temp)"
   ]
